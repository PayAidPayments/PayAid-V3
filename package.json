{
  "name": "payaid-v3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "verify:db": "tsx scripts/verify-database-connection-setup.ts",
    "check:vercel-env": "tsx scripts/check-vercel-env.ts",
    "check:postgrest": "tsx scripts/check-postgrest-status.ts",
    "dev": "next dev -p 3000 --webpack",
    "dev:websocket": "tsx server/websocket-voice-server.ts",
    "dev:telephony": "tsx server/telephony-audio-stream.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:websocket\"",
    "build": "prisma generate && next build --webpack",
    "build:typecheck": "next build --webpack",
    "build:with-db": "prisma generate && prisma db push --skip-generate --accept-data-loss && next build --webpack",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed-campaigns": "npx tsx prisma/seed-campaigns.ts",
    "db:seed-all": "npx tsx prisma/seed-all-sample-data.ts",
    "db:seed-news": "npx tsx prisma/seed-news-items.ts",
    "enable-all-modules": "npx tsx scripts/enable-all-modules.ts",
    "type-check": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc --noEmit",
    "type-check:quick": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc --noEmit --skipLibCheck",
    "check-before-push": "powershell -ExecutionPolicy Bypass -File scripts/check-before-push.ps1",
    "check-changed": "powershell -ExecutionPolicy Bypass -File scripts/check-changed-files.ps1",
    "verify-env": "npx tsx scripts/verify-env.ts",
    "check-vercel-logs": "powershell -ExecutionPolicy Bypass -File scripts/check-vercel-logs.ps1",
    "verify-env-and-logs": "powershell -ExecutionPolicy Bypass -File scripts/verify-env-and-logs.ps1",
    "verify-all": "npm run verify-env && npm run verify-env-and-logs",
    "setup:ai-influencer": "npx tsx scripts/setup-ai-influencer.ts",
    "verify:templates": "npx tsx scripts/verify-video-templates.ts",
    "setup:free-stack": "npx tsx scripts/setup-free-stack.ts",
    "setup:free-stack:start": "npx tsx scripts/setup-free-stack.ts --start-docker",
    "start:free-stack": "powershell -ExecutionPolicy Bypass -File scripts/start-free-stack.ps1",
    "test:health": "npx tsx scripts/test-system-health.ts",
    "test:queue": "npx tsx scripts/test-job-queue.ts",
    "test:monitoring": "npx tsx scripts/test-monitoring.ts",
    "setup:load-tests": "npx tsx scripts/load-test-setup.ts",
    "build:vercel": "powershell -ExecutionPolicy Bypass -File scripts/build-vercel-local.ps1",
    "build:local": "prisma generate && next build --webpack",
    "clean:build": "powershell -ExecutionPolicy Bypass -File scripts/clean-build.ps1",
    "pre-push": "powershell -ExecutionPolicy Bypass -File scripts/pre-push-check.ps1",
    "prebuild": "prisma generate",
    "build:vercel-check": "powershell -ExecutionPolicy Bypass -File scripts/pre-push-check.ps1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
