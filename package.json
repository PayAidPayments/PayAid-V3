    "y-websocket": "^3.0.0",
    "yjs": "^13.6.28",
    "zod": "^3.23.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/pdf-parse": "^1.1.5",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.0",
    "pg": "^8.16.3",
    "tsx": "^4.21.0"
  }
}
